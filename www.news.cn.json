[{
  "path": "all",
  "input": {
    "title": {
      "selector": ".title",
      "type": "text"
    }, 
    "source": {
      "selector": ".source",
      "type": "text"
    },
    "content": {
      "selector": "#detail",
      "type": "html"
    }
  },
  "output": {
    "title": "raw",
    "source": "raw",
    "author": "{{ content | strip_html | match: '（记者：(.*)）' }}",
    "content": "raw"
  }
}]
